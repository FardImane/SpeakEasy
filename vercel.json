{
    "version": 2,
    "buildCommand": "yarn build",
    "outputDirectory": "build",
    "public": "public",
    "routes": [
      {
        "src": "/api/newsletter/subscribe",
        "methods": ["POST"],
        "dest": "/api/newsletter.js"
      },
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ],
    "env": {
      "NODE_OPTIONS": "--openssl-legacy-provider"
    }
  }
  